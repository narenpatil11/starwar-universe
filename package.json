{
  "name": "etraveli",
  "version": "1.0.0",
  "author": "Narendra Patil",
  "description": "",
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "update": "rush update && rush check",
    "preinstall": "rush update && exit 0",
    "dev:app": "pnpm --filter @etraveli/starwar-universe-app dev",
    "dev:lib": "pnpm --filter @etraveli/ui-library dev",
    "test:app": "pnpm --filter @etraveli/starwar-universe-app test",
    "test:lib": "pnpm --filter @etraveli/ui-library test",
    "build:lib": "pnpm --filter @etraveli/ui-library build",
    "build:app": "pnpm --filter @etraveli/starwar-universe-app dev",
    "build": "rush build --verbose --to git:origin/main",
    "test": "pnpm run \"/^test:.*/\""
  },
  "keywords": [],
  "license": "ISC"
}
